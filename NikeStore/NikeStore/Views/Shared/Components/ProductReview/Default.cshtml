@model IEnumerable<ProductReview>

@foreach (var review in Model)
{
	<nav>
		<div class="AccountInfor">
			<img src="@review.?ImgUrl" alt="Alternate Text" />
			<div class="Account">
				<h3>
					@review.Reviewer
					@for (int i = 0; i <= review.Rating; i++)
					{
						<i class="fa-solid fa-star"></i>
					}
				</h3>
				<p>@review.CreatedAt</p>
			</div>
		</div>

		<div class="Content">
			<p>@review.Review</p>
		</div>
	</nav>
}